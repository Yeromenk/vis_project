
@{
    ViewData["Title"] = "Teacher";
}
<h1>@ViewData["Teacher"]</h1>
@using DataAccess.Model
@model List<Teacher>

<h1>Kontakty vsech ucitelu</h1>



<table class="table table-bordered table-hover">
    <thread>
        <tr>
            <th>Jmeno            </th>
            <th> Prijmeni</th>
            <th> Vek</th>
            <th> Email</th>
            <th>Predmet</th>
        </tr>

    </thread>
    <tbody>
        @foreach (Teacher t in Model)
        {
            <tr>
                <td> @t.first_name</td>
                <td> @t.last_name</td>
                <td> @t.age</td>
                <td>@t.email </td>
                 <td> @t.subject_name</td>
            </tr>
        }

    </tbody>



</table>

<div class=" clearfix"></div>